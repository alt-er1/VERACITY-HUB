    Name = "Visuals",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Credits = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--Gloabls and important locals

_G.ReverseFlashGodMode = false

local ThanosSkin = "Skin1"
local WandaSkin = "Skin1"
local SpeedSkin = 1
local ChadSkin = 1

-- functions

function Thanos()
    local args = {
        [1] = game:GetService("ReplicatedStorage").Skins.Thanos[tostring(ThanosSkin)],
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end 

function Wanda()
    local args = {
        [1] = game:GetService("ReplicatedStorage").Skins.Wanda[tostring(WandaSkin)],
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end

function Speed()
    local args = {
        [1] = game:GetService("ReplicatedStorage").SelectACharacter.Speed,
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end

function SkinSpeed()
    local args = {
        [1] = game:GetService("ReplicatedStorage").Skins.Speed[tostring(SpeedSkin)],
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end

function GigaChad()
    local args = {
        [1] = game:GetService("ReplicatedStorage").SelectACharacter.GigaChad,
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end

function GigaSkin()
    local args = {
        [1] = game:GetService("ReplicatedStorage").Skins.GigaChad[tostring(ChadSkin)],
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end

function Vecna()
    local args = {
        [1] = game:GetService("ReplicatedStorage").SelectACharacter.Vecnae,
        [2] = game:GetService("Players").LocalPlayer.PlayerGui.Menu.Frame.Select.SelectScript
    }
    
    game:GetService("ReplicatedStorage").SelectACharacter.ChangeChar:FireServer(unpack(args))
end

function ReverseGodMode()

    while _G.ReverseFlashGodMode == true do
        if _G.ReverseFlashGodMode == true then
            task.wait(2)
            local A_1 = game:GetService("Players").ReyZened
            local Event = game:GetService("Workspace").ReyZened.ReverseFlash.Phase
            Event:FireServer(A_1)
        end
    end
end
-- Gui stuff 

-- Characters

chars:AddSection({
	Name = "Thanos"
})
  
chars:AddDropdown({
	Name = "Skin",
	Default = "Default",
	Options = {"Default", "Comics", "Morty"},
	Callback = function(Value)
		
        if Value == "Default" then
            ThanosSkin = "Skin1"
        elseif Value == "Comics" then
            ThanosSkin = "Skin2"
        elseif Value == "Morty" then
            ThanosSkin = "Skin3"
        end
	end    
})

chars:AddButton({
	Name = "Thanos",
	Callback = function()
      	Thanos()
  	end    
})

chars:AddSection({
	Name = "Wanda"
})

chars:AddDropdown({
	Name = "Skin",
	Default = "Default",
	Options = {"Default", "60's", "838", "Casual", "Wendy"},
	Callback = function(Value)
		
        if Value == "Default" then
            WandaSkin = "Skin1"
        elseif Value == "60's" then
            WandaSkin = "Skin2"
        elseif Value == "838" then
            WandaSkin = "Skin3"
        elseif Value == "Casual" then
            WandaSkin = "Skin4"
        elseif Value == "Wendy" then
            WandaSkin = "Skin5"
        end
	end    
})

chars:AddButton({
	Name = "Wanda",
	Callback = function()
      	Wanda()
  	end    
})

chars:AddSection({
    Name = "Speed"
})

chars:AddDropdown({
    Name = "Skin",
    Default = "Default",
    Options = {"Default", "Ben", "Lowlife"},
    Callback = function(Value)
        
        if Value == "Default" then
            SpeedSkin = 1
        elseif Value == "Ben" then
            SpeedSkin = "Skin1"
        elseif Value == "Lowlife" then
            SpeedSkin = "Skin2"
        end

        print(SpeedSkin)
    end
})

chars:AddButton({
	Name = "Speed",
	Callback = function()
        if SpeedSkin == 1 then
            print('Default')
            Speed()
        else
            print'Skin'
            SkinSpeed()
        end
  	end    
})

chars:AddSection({
    Name = "GigaChad"
})

chars:AddDropdown({
    Name = "GigaChad",
    Default = "Default",
    Options = {"Default", "Femchad", "Saitama", "All Might", "Terrence"},
    Callback = function(Value)
        
        if Value == "Default" then
            ChadSkin = 1
        elseif Value == "Femchad" then
            ChadSkin = "Skin1"
        elseif Value == "Saitama" then
            ChadSkin = "Skin2"
        elseif Value == "All Might" then
            ChadSkin = "Skin3"
        elseif Value == "Terrence" then
            ChadSkin = "Skin4"
        end
    end
})

chars:AddButton({
	Name = "GigaChad",
	Callback = function()
        if ChadSkin == 1 then
            GigaChad()
        else
            GigaSkin()
        end
  	end    
})

chars:AddSection({
    Name = "Vecna"
})

chars:AddDropdown({
    Name = "Skin",
    Default = "Default",
    Options = {"Default"},
    Callback = function(Value)
        
        if Value == "Default" then
            
        end
    end
})

chars:AddButton({
	Name = "Vecna",
	Callback = function()
        Vecna()
  	end    
})

Toggles:AddSection({
	Name = "Reverse Flash"
})

Toggles:AddToggle({
	Name = "Reverse Flash God Mode",
	Default = false,
	Callback = function(Value)
		_G.ReverseFlashGodMode = Value
        ReverseGodMode()
	end    
})

Toggles:AddLabel("Doesn't work on some moves!")

Misc:AddButton({
	Name = "Server Hop",
	Callback = function()
        
	local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()

	module:Teleport(game.PlaceId)
	
  	end    
})

-- Visuals
Visuals:AddSlider({
	Name = "Time Of Day",
	Min = 0.1,
	Max = 24,
	Default = game.Lighting.ClockTime,
	Color = Color3.fromRGB(60, 234, 176),
	Increment = 0.1,
	ValueName = "Hours",
	Callback = function(Value)
		game.Lighting.ClockTime  = Value
	end    
})

Visuals:AddToggle({
    Name = "DOF",
    Default = true,

    Callback = function(Value)
        game.Lighting.DepthOfField.Enabled = Value
    end
})

Visuals:AddSlider({
	Name = "Field Of View",
	Min = 10,
	Max = 120,
	Default = game.Workspace.Camera.FieldOfView,
	Color = Color3.fromRGB(60, 234, 176),
	Increment = 1,
	ValueName = "FOV",
	Callback = function(Value)
		game.Workspace.CurrentCamera.FieldOfView  = Value
	end    
})

